#include <iostream>

using namespace std;

class Node{
    public:
    int data;
    Node* next;
    
    Node(int val){
        data=val;
        next=NULL;
    }
};

void insertatbeginning(Node* &head, int val){
    Node* n = new Node(val);
    n->next=head;
    head=n;
} 

void printList(Node* &head){
    Node* temp = head;
    while(temp!=NULL){
        cout<<temp->data<<"->";
        temp=temp->next;
    }
    cout<<"NULL"<<endl;
}

int countkey(Node* &head,int key){
    int count =0;
    Node* temp=head;
    while(temp!=NULL){
        if(key==temp->data){
            count++;
        }
        temp=temp->next;
    }
    return count;
}
int main()
{
Node* head=NULL;

insertatbeginning(head,4);
insertatbeginning(head,3);
insertatbeginning(head,3);
insertatbeginning(head,3);
insertatbeginning(head,3);
insertatbeginning(head,2);
insertatbeginning(head,1);
printList(head);
int count = countkey(head,3);
cout<<count;

    return 0;
}
